{
    "buildCommand": "npm run build",
    "outputDirectory": "dist",
    "framework": "vite",
    "rewrites": [
        {
            "source": "/zoom/(.*)",
            "destination": "/index.html"
        }
    ],
    "headers": [
        {
            "source": "/zoom/(.*)",
            "headers": [
                {
                    "key": "X-Frame-Options",
                    "value": "ALLOWALL"
                },
                {
                    "key": "Content-Security-Policy",
                    "value": "frame-ancestors 'self' https://*.zoom.us https://*.zoom.com"
                }
            ]
        }
    ]
}
